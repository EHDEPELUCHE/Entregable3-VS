pipeline {
    agent docker{
        image 'python:3.8'
    } 
    stages {
        stage('Build') { 
            steps {
                sh 'python3 -m py_compile https://github.com/EHDEPELUCHE/Entregable3-VS/tree/main/sources/add2vals.py https://github.com/EHDEPELUCHE/Entregable3-VS/tree/main/sources/calc.py' 
                stash(name: 'compiled-results', includes: 'https://github.com/EHDEPELUCHE/Entregable3-VS/tree/main/sources/*.py*') 
            }
        }
    }
}
